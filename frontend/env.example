# API Configuration
VITE_API_BASE_URL=http://localhost:5012/api
VITE_APP_NAME=HotelsOnWeb

# Environment
VITE_NODE_ENV=development

# External Services (if you plan to integrate with maps, analytics, etc.)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
VITE_ANALYTICS_ID=your_analytics_id

# Feature Flags (for enabling/disabling features)
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=true

# Image Upload Configuration
VITE_MAX_IMAGE_SIZE=5242880
VITE_ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/webp

# Payment Configuration (if you plan to add payment features)
VITE_STRIPE_PUBLIC_KEY=your_stripe_public_key
VITE_PAYMENT_CURRENCY=USD

# Social Login (if you plan to add OAuth)
VITE_GOOGLE_CLIENT_ID=your_google_client_id
VITE_FACEBOOK_APP_ID=your_facebook_app_id 